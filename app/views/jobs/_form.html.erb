<%= form_for job do |f| %>
    <div class="form-group">
      <%= f.label :title, 'Título' %>
      <%= f.text_field :title %>
    </div>
    <div class="form-group">
      <%= f.label :description, 'Descrição' %>
      <%= f.text_field :description %>
    </div>
    <div class="form-group">
      <%= f.label :desired_skill, 'Habilidades desejadas' %>
      <%= f.text_field :desired_skill %>
    </div>
    <div class="form-group">
      <%= f.label :salary_range, 'Faixa Salarial' %>
      <%= f.text_field :salary_range %>
    </div>
    <div class="form-group">
      <%= f.label :level, 'Nível' %>
      <%= f.text_field :level %>
    </div> 
    <div class="form-group">
      <%= f.label :deadline, 'Data limite' %>
      <%= f.text_field :deadline %>
    </div> 
    <div class="form-group">
      <%= f.label :region, 'Região' %>
      <%= f.text_field :region %>
    </div> 
    <%= f.submit 'Enviar' %>
<% end %>